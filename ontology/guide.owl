<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/ontologies/guide#"
     xml:base="http://www.semanticweb.org/ontologies/guide"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/ontologies/guide"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ontologies/guide#destroy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#destroy">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/guide#destroyed_by"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/guide#destroyed_by -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#destroyed_by"/>
    


    <!-- http://www.semanticweb.org/ontologies/guide#eaten_by -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#eaten_by">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/guide#eats"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/guide#eats -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#eats">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/guide#has -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#has"/>
    


    <!-- http://www.semanticweb.org/ontologies/guide#hates -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#hates"/>
    


    <!-- http://www.semanticweb.org/ontologies/guide#hitchhikes -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#hitchhikes">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/guide#is_friend_of -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#is_friend_of">
        <owl:equivalentProperty>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/guide#is_friend_of"/>
            </rdf:Description>
        </owl:equivalentProperty>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/guide#is_from -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#is_from"/>
    


    <!-- http://www.semanticweb.org/ontologies/guide#is_relative_of -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#is_relative_of">
        <owl:equivalentProperty>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/guide#is_relative_of"/>
            </rdf:Description>
        </owl:equivalentProperty>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/guide#likes -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#likes"/>
    


    <!-- http://www.semanticweb.org/ontologies/guide#lives -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#lives"/>
    


    <!-- http://www.semanticweb.org/ontologies/guide#produced_by -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#produced_by">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/guide#produces"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/guide#writes"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/guide#produces -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#produces">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/guide#writen_by"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/guide#travels_to -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#travels_to"/>
    


    <!-- http://www.semanticweb.org/ontologies/guide#writen_by -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#writen_by">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/guide#writes"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/guide#writes -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/guide#writes">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ontologies/guide#arts -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#arts">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/ontologies/guide#life"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/ontologies/guide#robot"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#life"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#robot"/>
            </owl:Restriction>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#book -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#book">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#hitchhiker"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#human"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#robot"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#vogon"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/guide#arts"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#dolphin -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#dolphin">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#eats"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#fish"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/guide#fauna"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#fauna -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#fauna">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/guide#life"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/guide#human"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/guide#vogon"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#fish -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#fish">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#eaten_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#dolphin"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/guide#fauna"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#god -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#god">
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/guide#life"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#hitchhiker -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#hitchhiker">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#hitchhikes"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#spaceship"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#lives"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#planet"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#lives"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#star_system"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produces"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#tools"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#writes"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#arts"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/guide#life"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#human -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#human">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#is_from"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/ontologies/guide#earth"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/guide#life"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/guide#vogon"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#life -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#life">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#is_from"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/ontologies/guide#planet"/>
            </owl:Restriction>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#mice -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#mice">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/guide#fauna"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#music -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#music">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#dolphin"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#hitchhiker"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#human"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#robot"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#vogon"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/guide#arts"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#planet -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#planet">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/guide#universe"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/guide#star_system"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#poetry -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#poetry">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#human"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#robot"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#vogon"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/guide#arts"/>
        <rdfs:comment>&quot;Poetry, well written, can be a spiritually uplifting experience. Badly written, it can be an experience of buttock-clenching horror.

The third worst poetry in the universe is written by Vogons, and frequently used as a form of torture.

The absolute worst poetry was written by Paula Nancy Millstone Jennings. It involved decaying swans. Luckily, it was destroyed during the demolition of the Earth.

Examples of good, if long, poetry can be heard on the planet of Golgafrincham, home to the great circling poets of Arium.&quot;

[Source: http://www.bbc.co.uk/cult/hitchhikers/guide/poetry.shtml]</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#robot -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#robot">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/guide#tools"/>
        <rdfs:comment xml:lang="en">&quot;A robot is a mechanical or virtual artificial agent, usually an electro-mechanical machine that is guided by a computer program or electronic circuitry. The Encyclopedia Galactica defines a robot as &apos;a mechanical apparatus designed to do the work of a man&apos;. The marketing division of the Sirius Cybernetics Corporation defines a robot as &apos;Your Plastic Pal Who&apos;s Fun To Be With&apos;.&quot;

[Description from the Douglas Adams&apos; book &quot;The Hitchhiker&apos;s Guide to the Galaxy&quot; (1979)]</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#spaceship -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#spaceship">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#travels_to"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#universe"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/guide#tools"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#star_system -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#star_system">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/guide#universe"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#tools -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#tools">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#human"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#vogon"/>
            </owl:Restriction>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#universe -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#universe">
        <owl:equivalentClass>
            <owl:Class>
                <owl:complementOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#produced_by"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#god"/>
                    </owl:Restriction>
                </owl:complementOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#has"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#life"/>
            </owl:Restriction>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/guide#vogon -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/guide#vogon">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#hates"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/guide#hitchhiker"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/guide#is_from"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/ontologies/guide#vogosphere"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/guide#life"/>
        <rdfs:comment xml:lang="en">&quot;Vogons are extremely ugly, extremely officious, and generally not much fun to be around.

They emerged from the seas of the planet Vogsphere, and gave up on evolving there and then. Only their stubbornness allowed them to survive.

They generally become bureaucrats in the galactic government. Their unpleasant demeanour makes them ideally suited to such employment.

One such Vogon is Prostetnic Vogon Jeltz, of the Vogon Constructor fleet. Having destroyed the Earth in order to make way for a hyperspace bypass, he then proceded to read some of his poetry to two helpless victims.

It is hard to hitch lifts on Vogon ships - it is only made possible by the Dentrassi cooks employed by Vogon fleets.&quot;

[Source: http://www.bbc.co.uk/cult/hitchhikers/guide/vogon.shtml]</rdfs:comment>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ontologies/guide#Alpha_Centauri -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/ontologies/guide#Alpha_Centauri">
        <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/guide#star_system"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/ontologies/guide#Arthur_Dent -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/ontologies/guide#Arthur_Dent">
        <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/guide#hitchhiker"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/guide#human"/>
        <is_friend_of rdf:resource="http://www.semanticweb.org/ontologies/guide#Trillian_Astra"/>
        <is_friend_of rdf:resource="http://www.semanticweb.org/ontologies/guide#Zaphod_Beeblebrox"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/ontologies/guide#Beetelgeuse -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/ontologies/guide#Beetelgeuse">
        <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/guide#star_system"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/ontologies/guide#Disaster_Area -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/ontologies/guide#Disaster_Area">
        <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/guide#music"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/ontologies/guide#Encyclopedia_Galactica -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/ontologies/guide#Encyclopedia_Galactica">
